body{
text-align:center;	
}

header{
width:100%;
height:150px;
position:relative;
color:blue;	
z-index:10;
}

header .navbar .container-fluid .collapse ul{
background:white;	
}

header .navbar .container-fluid .collapse ul li:hover{
background:lightyellow;	
}

header .navbar .container-fluid .collapse ul li{
width:90px;	
color:blue;	
}

a:link{
text-decoration:none;
font-size:20px;	
font-style:italic;
font-family:forte;
color: #007bff;
text-decoration: none;
background-color: transparent;

}

a:hover{
color:orange;	
}

h3,h1,h4{
font-family:Palatino Linotype;

}

main{
width:100%;
height:auto;

font-family:Calibri (Body);
}



.myprojects .container .row{
width:1500px;
height:250px;	
} 

footer{
width:100%;	
height:300px;

}

main .resume h4{
color:blue;	
font-family:Footlight MT Light;
}

main .resume h5,h6{
font-family:Footlight MT Light;
}

.list-unstyled{
height:50px;
display:flex;
justify-content:center;

}	

.list-unstyled a{
height:100px;
width:100px;
border-radius:50%;
text-align:center;	
margin:10px;
box-shadow:1px 4px 2px 2px black;

}

.list-unstyled a {
transition:all 0.3s linear;	
}

.list-unstyled a:hover{
font-size:30px;	
}

.fa-envelope{
color:blue;	
}
.fa-instagram{
color:purple;	
}

.fa-whatsapp{
color:green;	
}

.fa-linkedin{
color:blue;	
}

.preloader svg:hover{
color:green;	
}